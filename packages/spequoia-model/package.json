{
  "name": "spequoia-model",
  "version": "1.0.0",
  "description": "Spequoia models as TypeScript interfaces and JSON schemas",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf schema",
    "build": "npx ts-json-schema-generator --path src/model/spequoia.model.ts --type SpequoiaDocument --additional-properties --out schema/spequoia.json",
    "tsdoc": "tsdoc --src=src/model/* --types --noemoji"
  },
  "author": "pckerneis",
  "license": "MIT",
  "dependencies": {
    "ts-json-schema-generator": "^2.4.0"
  },
  "devDependencies": {
    "tsdoc-markdown": "^1.2.0"
  }
}
